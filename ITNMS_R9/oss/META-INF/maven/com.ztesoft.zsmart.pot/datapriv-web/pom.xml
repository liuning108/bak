<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.ztesoft.zsmart.pot</groupId>
        <artifactId>datapriv-parent</artifactId>
        <version>9.1.0-SNAPSHOT</version>
        <relativePath>../pom.xml</relativePath>
    </parent>
    <artifactId>datapriv-web</artifactId>
    <packaging>war</packaging>
    <name>datapriv-web</name>
    <url>http://maven.apache.org</url>
    <dependencies>
        <!--portal-server/portal-client与 portal-web，如果依赖的portal.war排除了lib目录下面的jar，这里就需要主动引入 -->
        <dependency>
            <groupId>com.ztesoft.zsmart.pot</groupId>
            <artifactId>datapriv-service-api</artifactId>
            <version>${project.parent.version}</version>
        </dependency>
        <dependency>
            <groupId>com.ztesoft.zsmart.pot</groupId>
            <artifactId>datapriv-service-impl</artifactId>
            <version>${project.parent.version}</version>
        </dependency>
        <dependency>
            <groupId>com.ztesoft.zsmart.pot</groupId>
            <artifactId>portal-web</artifactId>
        </dependency>
        <dependency>
            <groupId>com.ztesoft.zsmart.pot</groupId>
            <artifactId>portal-service-impl</artifactId>
        </dependency>
        <dependency>
            <groupId>com.ztesoft.zsmart.pot</groupId>
            <artifactId>portal-common</artifactId>
        </dependency>
        <dependency>
            <groupId>com.ztesoft.zsmart.pot</groupId>
            <artifactId>stafforg-service-impl</artifactId>
        </dependency>
        <dependency>
            <groupId>javax.servlet</groupId>
            <artifactId>javax.servlet-api</artifactId>
            <scope>provided</scope>
        </dependency>
    </dependencies>
    <build>
        <finalName>datapriv-web</finalName>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-war-plugin</artifactId>
                <configuration>
                    <failOnMissingWebXml>false</failOnMissingWebXml>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>